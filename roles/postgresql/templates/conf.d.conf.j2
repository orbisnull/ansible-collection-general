#jinja2: trim_blocks: True, lstrip_blocks: True
# {{ ansible_managed }}
{% for  key, value in psql_custom_conf.items() %}
    {% if value is iterable and (value is not string and value is not mapping) %}
        {{ key }} = {{ value |  join(", ") }}
    {% else %}
        {{ key }} = {{ value }}
    {% endif %}
{% endfor %}
